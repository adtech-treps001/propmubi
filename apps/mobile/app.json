{
    "expo": {
        "name": "PropMubi",
        "slug": "propmubi",
        "version": "1.0.0",
        "scheme": "propmubi",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            }
        },
        "plugins": [
            "expo-router"
        ]
    }
}