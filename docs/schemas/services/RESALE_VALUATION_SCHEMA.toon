# Propmubi Property Value Assessment (Resale & Rental)
# Format: TOON
# Goal: Automated Valuation Model (AVM) and Rental Yield prediction based on granular data.

# -----------------------------------------------------------------------------
# 1. VALUATION AGGREAGTORS
# -----------------------------------------------------------------------------
ValuationPartnerEnum = [
  "Liases_Foras",
  "PropStack",
  "MagicBricks_PropWorth",
  "Housing_Data_Science",
  "Jones_Lang_LaSalle_JLL",
  "CBRE_Valuation",
  "Propmubi_AVM_Engine"
]

# -----------------------------------------------------------------------------
# 2. PROPERTY SCORECARD (The Input)
